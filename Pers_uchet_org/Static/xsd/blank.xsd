				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="part_condition_name" type="xsd:string"/> 
			<xsd:element name="staj_base_id" default="301">
				<xsd:simpleType>
					<xsd:restriction base="xsd:nonNegativeInteger">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="399"/>
