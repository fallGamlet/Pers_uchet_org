	<xsd:complexType name="RecordType">
		<xsd:sequence>
			<xsd:element name="dop_code_id" default="501">
				<xsd:simpleType>
					<xsd:restriction base="xsd:nonNegativeInteger">
						<xsd:minInclusive value="0"/>
